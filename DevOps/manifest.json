    },    "Application": {
        "Name" : "/operations/database-mssql",
   
        "Deployment": {
            "Order": [
                "/Principals/Roles/*",
                "/Principals/Users/*",
                "/Rules/*",
                "/Partition Functions/*",
                "/Partition Schemes/*",
                "/Schema/*",
                "/Scripts/PreDeployment/*",
                "/Sequences/*",
                "/User Defined Data Types/*",
                "/User Defined Table Types/*",
                "/Triggers/*",
                "/User Defined Functions/*",
                "/Views/*",
                "/Stored Procedures/*",
                "/Synonyms/*",
                "/Scripts/PostDeployment/*"
            ],
            "ReRunnable": [
                "/Views/*",
                "/User Defined Functions/*",
                "/Stored Procedures/*"
            ],
            "Exclude": [
                "/Tables/*",
                "*/Manual/*",
                "*/Archive/*"
            ]
        },        "SSRS" : {
            "RootFolders": [ "/SSRS" ],
            "DataSourceMapping": {
                "CORE" : ["Core", "DEV3", "dsDev03", "DataSource1", "DSPerformance", "dsPerformance", "DEV03", "dsDev"],
                "WEBSERVICE" : ["AttributionService"]               
            }
        },
        "SSIS" : {
            "RootFolders": [ "/SSIS" ]
        }
    }
